{
    "executionRoleArn": "arn:aws:iam::498910579498:role/ecsTaskExecutionRole",
    "family": "catalogue",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "containerDefinitions": {
        "name": "catalogue",
        "image": "498910579498.dkr.ecr.us-east-1.amazonaws.com/catalogue:latest",
        "portMappings": [
            {
                "containerPort": 9000,
                "hostPort": 9000,
                "protocol": "tcp"
            }
        ],
        "essential": true
    },
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256"
  }